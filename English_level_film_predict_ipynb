        "  if updated_df.loc[i, 'Level'] == 'A2/A2+':\n",
        "    updated_df.loc[i, 'Level'] = 'A2'\n",
        "  elif updated_df.loc[i, 'Level'] == 'B1, B2':\n",
        "    updated_df.loc[i, 'Level'] = 'B1'\n",
        "  elif updated_df.loc[i, 'Level'] == 'А2':\n",
        "    updated_df.loc[i, 'Level'] = 'A2'\n",
